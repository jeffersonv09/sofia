<div class="container" id="main-content">
  <% if(flash && flash.err){ %>
  <ul class="alert alert-success">
    <% Object.keys(flash.err).forEach(function(error){ %>
    <li><%- JSON.stringify(flash.err[error]) %></li>
    <% }) %>
  </ul>
  <% } %>
  <div class="row">
    <div class="col-sm-8 col-xs-12 col-sm-offset-2">
      <form class="form-horizontal formulario" action="/etiquetaCategoria/crear" method="POST">
      	<div class="form-header">
      		<h3>Categoría de etiqueta</h3>
      	</div>
      	<div class="form-body">
          <div class="control-group">
            <div class="input-container">
              <input type="input" name="etiquetaCategoriaNombre" id="etiquetaCategoriaNombre" required="required"/>
              <label for="etiquetaCategoriaNombre">Nombre de categoría</label>
              <div class="bar"></div>
            </div>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>